{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/projects",
        "destination": "/projects.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
